{
  "cli": { "version": ">= 5.0.0" },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": { "simulator": false },
      "env": { "EXPO_PUBLIC_API_URL": "https://nexo-api-staging.vercel.app" }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "ios": { "simulator": false },
      "env": { "EXPO_PUBLIC_API_URL": "https://nexo-api.vercel.app" }
    }
  },
  "submit": {
    "production": {
      "ios": { "appleId": "TU_EMAIL", "ascAppId": "TU_APP_ID" }
    }
  }
}
